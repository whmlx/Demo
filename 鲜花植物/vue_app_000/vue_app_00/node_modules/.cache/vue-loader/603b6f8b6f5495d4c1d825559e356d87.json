\n            </mt-tab-item>\r\n        </mt-tabbar>\r\n        <!-- 底部导航栏  end-->\r\n    </div>\r\n</template>\r\n<script>\r\n//1.引入子组件 TitleBar.vue\r\nimport Index from './common/Index.vue'\r\nimport Product from './common/Product.vue'\r\nimport TabbarIcon from './common/TabbarIcon.vue'\r\nimport me from './common/me.vue'\r\nimport yy from './common/xiangmu.vue'\r\nexport default {\r\n    data() {\r\n        return {\r\n            //保存当前显示子面板id\r\n            active:\"tab0\",\r\n            //创建数组保存tabbar状态\r\n            list:[\r\n                {isSelect:true},//首页\r\n                {isSelect:false},//更多植物\r\n                {isSelect:false},//预约\r\n                {isSelect:false},//我的\r\n            ],\r\n            \r\n        }\r\n    }, \r\n    methods: {\r\n        changeState(idx){\r\n            for(var i=0;i<this.list.length;i++){\r\n                if(i==idx){\r\n                    sessionStorage.setItem(\"i\",idx);\r\n                    this.list[i].isSelect=true;\r\n                }else{\r\n                    this.list[i].isSelect=false;\r\n                }\r\n            }\r\n        },\r\n        \r\n    },\r\n    created(){\r\n        var idx=sessionStorage.getItem(\"i\")\r\n        if(idx==null){\r\n            idx=0;\r\n            this.changeState(idx);\r\n            this.active=`tab${idx}`;\r\n        }else{\r\n            this.changeState(idx);\r\n            this.active=`tab${idx}`;\r\n        }\r\n    },\r\n    //2.注册子组件\r\n    components: {\r\n        \"index\":Index,\r\n        \"product\":Product,\r\n        \"tabbaricon\":TabbarIcon,\r\n        \"me\":me,\r\n        \"yy\":yy\r\n    },\r\n}\r\n</script>\r\n<style scoped>\r\n/* 1.修改tabbar中默认-灰和选择文字-绿 */\r\n/* 文体颜色 */\r\n/* 默认文体颜色 */\r\n.mint-tabbar{\r\n    height:50px;\r\n    box-sizing: border-box;\r\n}\r\n.mint-tabbar>.mint-tab-item{\r\n    color:#999;\r\n}\r\n/* 选中文字颜色 */\r\n.mint-tabbar>.mint-tab-item.is-selected{\r\n    background-color: transparent;\r\n    color: rgb(87, 